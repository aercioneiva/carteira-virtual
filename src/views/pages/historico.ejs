<%- contentFor('body') %>
<div class="row">
    <div class="col-md-12">
        <h1>Historico Movimentações</h1>

        <table class="table">
                <thead>
                  <tr>
                    <th scope="col">Tipo</th>
                    <th scope="col">Categoria</th>
                    <th scope="col">Valor</th>
                    <th scope="col">Observacao</th>
                    <th scope="col">Data</th>
                  </tr>
                </thead>
                <tbody>
                    <% mov.forEach((m) => { %>
                        <tr>
                            <td><%= m.tipo %></td>
                            <td><%= m.categoria %></td>
                            <td><%= m.valor %></td>
                            <td><%= m.observacao %></td>
                            <td><%= m.createdAt %></td>
                        </tr>
                    <% }) %>
                  
                  
                </tbody>
              </table>


        
    </div>
</div>


